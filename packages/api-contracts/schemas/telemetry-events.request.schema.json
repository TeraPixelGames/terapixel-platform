{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://terapixel.dev/contracts/telemetry-events.request.schema.json",
  "title": "TelemetryEventsRequest",
  "type": "object",
  "required": ["game_id", "events"],
  "properties": {
    "game_id": {
      "type": "string",
      "minLength": 1
    },
    "profile_id": {
      "type": "string",
      "minLength": 1
    },
    "session_id": {
      "type": "string",
      "minLength": 1
    },
    "now_seconds": {
      "type": "integer",
      "minimum": 1
    },
    "events": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["event_name"],
        "properties": {
          "event_name": {
            "type": "string",
            "minLength": 1
          },
          "event_time": {
            "type": "integer",
            "minimum": 1
          },
          "seq": {
            "type": "integer",
            "minimum": 0
          },
          "properties": {
            "type": "object",
            "additionalProperties": true
          }
        },
        "additionalProperties": false
      }
    }
  },
  "additionalProperties": false
}
